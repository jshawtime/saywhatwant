{
  "name": "say-what-want",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "lsof -ti:3000,3001 | xargs kill -9 2>/dev/null; PORT=3000 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "test:setup": "node scripts/test-setup.js",
    "deploy": "npm run build && wrangler pages deploy out --project-name=say-what-want",
    "worker:dev": "cd workers && wrangler dev",
    "worker:deploy": "cd workers && wrangler deploy",
    "manifest:generate": "node scripts/r2-manifest-generator.js",
    "manifest:local": "node scripts/local-video-manifest-generator.js"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.5.3",
    "tailwindcss": "^3.4.6",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "lucide-react": "^0.400.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "wrangler": "^3.0.0"
  }
}
